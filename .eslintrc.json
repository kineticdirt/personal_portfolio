{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "no-unused-vars": ["warn"],
      "no-console": "off",
      "indent": ["error", 4],
      "linebreak-style": ["error", "unix"],
      "comma-dangle": ["error", "never"]
    }
  }
  